\ProvidesPackageRCS $Header: /Users/joseph/Library/texmf/tex/latex/beamer/base/themes/color/beamercolorthemehi.sty,v d02a7cf4d8ae 2010/06/17 09:11:41 rivanvx $


\mode<presentation>

\definecolor{hiorange}{RGB}{235,77,32}
\definecolor{hiblue}{RGB}{4,6,76}

\setbeamercolor{structure}{fg=hiblue}

\setbeamercolor{palette primary}{fg=hiorange,bg=hiblue!70}
\setbeamercolor{palette secondary}{fg=hiorange,bg=hiblue!80}
\setbeamercolor{palette tertiary}{fg=hiorange,bg=hiblue!90}
\setbeamercolor{palette quaternary}{fg=hiorange,bg=hiblue}

\setbeamercolor{titlelike}{parent=palette quaternary}

\setbeamercolor{block title}{fg=hiorange,bg=hiblue}
\setbeamercolor{block title alerted}{use=alerted text,fg=hiorange,bg=alerted 
text.fg!75!bg}
\setbeamercolor{block title example}{use=example text,fg=hiorange,bg=example 
text.fg!75!bg}

\setbeamercolor{block body}{parent=normal text,use=block title,bg=block title.bg!25!bg}
\setbeamercolor{block body alerted}{parent=normal text,use=block title alerted,bg=block title alerted.bg!25!bg}
\setbeamercolor{block body example}{parent=normal text,use=block title example,bg=block title example.bg!25!bg}

\setbeamercolor{sidebar}{bg=hiblue!70}
  
\setbeamercolor{palette sidebar primary}{fg=hiorange}
\setbeamercolor{palette sidebar secondary}{fg=hiorange!75}
\setbeamercolor{palette sidebar tertiary}{fg=hiorange!75}
\setbeamercolor{palette sidebar quaternary}{fg=hiorange}

\setbeamercolor*{separation line}{}
\setbeamercolor*{fine separation line}{}

\mode
<all>
