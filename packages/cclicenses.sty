\newcount\hpos
\newcount\vpos
\newcommand{\SA@arrow}{
\hdim\chardim
\ifdim\hdim<1.5pt \hdim=1.5pt\fi
\setlength\unitlength{1sp}
\begin{picture}(0,0)
  \hpos=0
  \vpos\hdim
  \loop
    \put(\hpos,\vpos){
      \rule[0mm]{\hdim}{1000sp}
    }
    \advance\htmp by 1000sp
    \hpos\htmp
    \advance\vpos by -1000
    \advance\hdim by -2000sp
  \ifnum\vpos > 0 \repeat
\end{picture}}
